#content-results {

	section {
		text-align: left;

		h2 {
			text-align: center;
		}

		ul {
			// padding: 20px 0px;

			li {
				padding: 10px 10px;
				margin: 5px;
				border-top: 1px solid #ddd;

				p {
					color: #fff;
				}

				&.cut {
					background: #bd667e;
				}

				&.add {
					background: #9dbd66;
				}

				&:first-child {
					border-top: none;
				}
			}
		}
	}
}
